<module>
	<inherits name="com.google.gwt.user.User"/>
	<inherits name="com.google.gwt.jsio.JSIO"/>
	<entry-point class="com.google.gwt.sample.hello.client.Hello"/>
	<!-- Replace the string XYZZY with a Google Maps API key obtained from:
		http://www.google.com/apis/maps/signup.html

	     To make hosted mode work, specify the web site URL as
		http://localhost:8888/com.google.gwt.sample.hello.Hello/
	-->
	<script src="http://maps.google.com/maps?file=api&amp;v=2&amp;key=XYZZY" />
</module>
